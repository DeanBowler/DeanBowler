{
  "headers": [
    {
      "source": "/(.*).(jpe?g|png|ico|webp|svg|xml|ttf|woff2?)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/git",
      "destination": "https://github.com/DeanBowler"
    },
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/dean-bowler-875a7323"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/SpencerBatwick"
    }
  ]
}
